# vim: foldmethod=marker foldmarker=#{,#}
# see `man zshoptions`
########################
# Changing Directories #
setopt AUTO_CD
setopt AUTO_PUSHD
setopt CDABLE_VARS
setopt PUSHD_IGNORE_DUPS
setopt PUSHD_SILENT
##############
#Expansion and Globbing
setopt GLOB_STAR_SHORT
setopt NUMERIC_GLOB_SORT
setopt RC_EXPAND_PARAM
##############
#Input/Output
setopt NOCLOBBER
setopt CORRECT
setopt INTERACTIVE_COMMENTS
