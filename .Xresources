!!{{{ Rxvt
!set up triple click grabs logical line
Rxvt.tripleclickwords:       false

!Font
Rxvt.font:     -xos4-terminus-medium-r-normal--14-140-72-72-c-80-iso10646-1
Rxvt.boldFont: -xos4-terminus-bold-r-normal--14-140-72-72-c-80-iso10646-1
Rxvt.letterSpace: 0

!! Font keybinds
!! 4x6. tiny font

!! Disable ctrl+shift shit
Rxvt.iso14755: false
Rxvt.iso14755_52: false

! set up fading on no focus
Rxvt.fading: 20%

! set pastable tabs
Rxvt.pastableTabs:  true

! No Scrolling
Rxvt.scrollBar: false
Rxvt.scrollTtyOutput: false
Rxvt.saveLines:0
Rxvt.scrollTtyKeypress: true

! No Borders
Rxvt.borderLess:     false
Rxvt.internalBorder: 0
Rxvt.externalBorder: 0

Rxvt.perl-ext-common: default,matcher,-option-popup,-selection-popup,confirm-paste
Rxvt.url-launcher:    palemoon

Rxvt.cutchars:        "`()'*<>[]{|â”‚}"

Rxvt.visualBell:     false
Rxvt.urgentOnBell:   true

!!{{{ Colors
Rxvt.transparent:    true

Rxvt.intensityStyles:true

Rxvt.tintColor:         #222222
Rxvt.underlineColor:    #0088ff
!! default bg/fg
Rxvt.background:    [50]#000000
